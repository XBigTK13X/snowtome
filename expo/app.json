{
  "expo": {
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/icon/android/tvbanner.png"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://bugsink.9914.us",
          "project": "snowstream",
          "organization": "Arctic"
        }
      ]
    ],
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.simplepathstudios.snowtome",
      "backgroundColor": "#000000",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.simplepathstudios.snowtome"
    },
    "splash": {
      "image": "./assets/icon/splash.png",
      "backgroundColor": "#9ec0e6"
    },
    "newArchEnabled": true,
    "name": "snowtome",
    "slug": "snowtome",
    "scheme": "com.simplepathstudios.snowtome",
    "icon": "./assets/icon/icon.png"
  }
}