{
  "expo": {
    "plugins": [
      [
        "@react-native-tvos/config-tv",
        {
          "androidTVBanner": "./assets/icon/android/tvbanner.png",
          "appleTVImages": {
            "icon": "./assets/images/icon-1280x768.png",
            "iconSmall": "./assets/images/icon-400x240.png",
            "iconSmall2x": "./assets/images/icon-800x480.png",
            "topShelf": "./assets/images/icon-1920x720.png",
            "topShelf2x": "./assets/images/icon-3840x1440.png",
            "topShelfWide": "./assets/images/icon-2320x720.png",
            "topShelfWide2x": "./assets/images/icon-4640x1440.png"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {}
        }
      ],
      "expo-router"
    ],
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.simplepathstudios.snowtome"
    },
    "ios": {
      "bundleIdentifier": "com.simplepathstudios.snowtome"
    },
    "splash": {
      "image": "./assets/icon/splash.png",
      "backgroundColor": "#9ec0e6"
    },
    "newArchEnabled": true,
    "name": "snowtome",
    "slug": "snowtome",
    "scheme": "com.simplepathstudios.snowtome",
    "icon": "./assets/icon/icon.png"
  }
}